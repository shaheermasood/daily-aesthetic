# EditorConfig - Consistent coding styles across editors and IDEs
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.{js,json}]
indent_size = 2

[*.{css,html}]
indent_size = 2
